<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        metadata-complete="false"
        version="3.0">
    <servlet>
        <servlet-name>domino</servlet-name>
        <servlet-class>controllers.DominoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>domino</servlet-name>
        <url-pattern>domino</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>showAll</servlet-name>
        <servlet-class>controllers.ShowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showAll</servlet-name>
        <url-pattern>get-domino</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>showChain</servlet-name>
        <servlet-class>controllers.ShowChainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showChain</servlet-name>
        <url-pattern>get-chain</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>chainName</servlet-name>
        <servlet-class>controllers.ChainNameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>chainName</servlet-name>
        <url-pattern>chain-name</url-pattern>
    </servlet-mapping>
</web-app>
